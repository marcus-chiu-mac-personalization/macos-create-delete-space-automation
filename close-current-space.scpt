-- values of horizontal position of close-space buttonset evenList to {212, 388, 564, 740, 916, 1092}set evenListSize to count of evenListset oddList to {120, 297, 474, 650, 826, 1002, 1178}set oddListSize to count of oddListtell application "System Events"	tell application "Mission Control" to launch	tell group 2 of group 1 of group 1 of process "Dock"		-- create new space (this expands the space docker in order to delete current space)		click (every button whose value of attribute "AXDescription" is "add desktop")		tell list 1			set spaceCount to count of buttons			set theMod to spaceCount mod 2			spaceCount						if theMod = 0 then				-- DELETE NEWLY CREATED SPACE				set ii to (evenListSize - spaceCount) / 2				set pos to item (ii + spaceCount) of evenList				set comman to "/usr/local/bin/cliclick kd:alt c:" & pos & ",30"				do shell script comman				-- DELETE CURRENT SPACE				set curSpaceUUID to do shell script "defaults read com.apple.spaces | grep 'uuid = ' | head -n 1 | tr -d '[:space:]' | cut -d '\"' -f2"				set curSpaceNum to do shell script "defaults read com.apple.spaces | grep 'uuid = ' | sed -n '1!p' | grep -n " & curSpaceUUID & " | cut -d ':' -f1"				set curSpaceNum to curSpaceNum as integer				set ii to (oddListSize - (spaceCount - 1)) / 2				set pos to item (ii + curSpaceNum) of oddList				set comman to "/usr/local/bin/cliclick kd:alt c:" & pos & ",30"				do shell script comman				-- CLICK ON PREVIOUS SPACE				delay 0.5				set curSpaceNum to curSpaceNum - 1				if curSpaceNum = 0 then					set curSpaceNum to 1				end if				set ii to (evenListSize - (spaceCount - 2)) / 2				set pos to item (ii + curSpaceNum) of evenList				set pos to pos + 50				set comman to "/usr/local/bin/cliclick kd:alt c:" & pos & ",50"				do shell script comman			else				-- DELETE NEWLY CREATED SPACE				set ii to (oddListSize - spaceCount) / 2				set pos to item (ii + spaceCount) of oddList				set comman to "/usr/local/bin/cliclick kd:alt c:" & pos & ",30"				do shell script comman				-- DELETE CURRENT SPACE				set curSpaceUUID to do shell script "defaults read com.apple.spaces | grep 'uuid = ' | head -n 1 | tr -d '[:space:]' | cut -d '\"' -f2"				set curSpaceNum to do shell script "defaults read com.apple.spaces | grep 'uuid = ' | sed -n '1!p' | grep -n " & curSpaceUUID & " | cut -d ':' -f1"				set curSpaceNum to curSpaceNum as integer				set ii to (evenListSize - (spaceCount - 1)) / 2				set pos to item (ii + curSpaceNum) of evenList				set comman to "/usr/local/bin/cliclick kd:alt c:" & pos & ",30"				do shell script comman				-- CLICK ON PREVIOUS SPACE				delay 0.5				set curSpaceNum to curSpaceNum - 1				if curSpaceNum = 0 then					set curSpaceNum to 1				end if				set ii to (oddListSize - (spaceCount - 2)) / 2				set pos to item (ii + curSpaceNum) of oddList				set pos to pos + 50				set comman to "/usr/local/bin/cliclick kd:alt c:" & pos & ",50"				do shell script comman			end if		end tell	end tellend tell